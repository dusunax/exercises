<section class="list">
    <h2>list</h2>
    <ul class="workList">
        <% data.forEach((workout)=>{ %>
        <li>
            <span>
                <%= workout.work_name %>
            </span>
            <strong class="weightText">
                <%= workout.work_weight %>kg
            </strong>
            <form class="btn_box" action="/update" method="post">
                <button class="btnWeight" name="btn" value="-5">-</button>
                <button class="btnWeight" name="btn" value="+5">+</button>
                <input type="hidden" name="wID" value="<%= workout._id %>">
                <input type="hidden" name="userID" value="<%= userID %>">
            </form>
        </li>
        <% }); %>
    </ul>
    <div class="btn_crud">
        <button name="add" onclick="btnClick(name)">추가</button>
        <!-- <button name="edit" onclick="btnClick(name)">수정</button> -->
        <button name="del" onclick="btnClick(name)">삭제</button>
    </div>
</section>