<section class="add modal">
  <h2>create page</h2>
  <form action="/add" method="post">
    <div class="input_area">
      <h5>운동:</h5>
      <input type="text" name="wName" autocomplete="off">
    </div>
    <div class="input_area">
      <h5>무게(kg):</h5>
      <input
        type="number"
        name="wWeight"
        autocomplete="off"
        min="5" step="5"
      >
    </div>
    <button name="button">추가하기</button>
    <button name="del" onclick="btnClose(name)">
        닫기
    </button>
    <input type="hidden" name="userID" value="<%= userID %>">
  </form>
</section>